node{
    tools {
        ant 'ant'
	jdk 'jdk8'
	}  
      
   stages {
        stage ('Initialization') {
	    steps {
	          echo "Hello Initializatio"
		  }
	  }
	stage ('Build') {
	     steps {
	          bat 'cd AntTestJavaProject & ant'
		  }

              post {
	         success {
		         echo "Successful"
		        }
                   }
	   }
	}
    }
